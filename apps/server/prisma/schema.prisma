generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Investment {
  id        String   @id @default(uuid())
  ticker    String
  date      DateTime
  quantity  Float
  price     Float
  createdAt DateTime @default(now()) @map("created_at")
}
